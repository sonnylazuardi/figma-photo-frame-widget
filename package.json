{
  "name": "Photo Frame",
  "version": "1.0.0",
  "description": "Photo Frame widget",
  "scripts": {
    "build": "webpack --mode production",
    "build-app": "esbuild src/app.tsx --bundle --outfile=dist/code.js --target=es6"
  },
  "author": "sonnylazuardi",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "@figma/widget-typings": "*",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "esbuild": "*",
    "esbuild-plugin-inline-import": "^1.0.4",
    "eslint": "^8.54.0",
    "typescript": "^4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./widget-src/tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "css-loader": "^6.2.0",
    "exifreader": "^4.21.1",
    "heic2any": "^0.0.4",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.6.0",
    "modern-screenshot": "^4.4.38",
    "postcss": "^8.4.21",
    "postcss-import": "^16.0.1",
    "postcss-loader": "^7.1.0",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^18.2.0",
    "react-lottie-player": "^1.5.6",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.5.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "tailwindcss": "^3.2.7",
    "autoprefixer": "^10.4.14"
  }
}